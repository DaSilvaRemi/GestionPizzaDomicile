                  <Font name="Source Code Pro Medium" size="12.0" />
               </font></TextField>
            <Label layoutX="275.0" layoutY="226.0" text="Adresse email">
               <font>
                  <Font name="Source Code Pro Medium" size="12.0" />
               </font></Label>
            <Label layoutX="275.0" layoutY="292.0" text="Mot de passe">
               <font>
                  <Font name="Source Code Pro Medium" size="12.0" />
               </font>
            </Label>
            <PasswordField fx:id="password_input" layoutX="275.0" layoutY="310.0" prefHeight="25.0" prefWidth="250.0" promptText="mot de passe">
