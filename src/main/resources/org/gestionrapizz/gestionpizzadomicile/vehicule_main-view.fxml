<VBox xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.gestionrapizz.gestionpizzadomicile.controller.VehiculeMainController">
    <children>
        <Pane maxHeight="500.0" maxWidth="800.0" minHeight="500.0" minWidth="800.0" prefHeight="500.0" prefWidth="800.0">
         <children>
            <TableView fx:id="vehicule_main_tableview" layoutX="450.0" layoutY="61.0" prefHeight="397.0" prefWidth="311.0">
              <columns>
                <TableColumn fx:id="vehicule_main_type_tableColumn" minWidth="0.0" prefWidth="121.33334350585938" text="Type" />
                <TableColumn fx:id="vehicule_main_immatriculation_tableColumn" minWidth="0.0" prefWidth="187.6666259765625" text="Immatriculation" />
              </columns>
            </TableView>
            <Button layoutX="57.0" layoutY="163.0" mnemonicParsing="false" onMouseClicked="#onCreateVehiculeButtonClick" prefHeight="67.0" prefWidth="311.0" text="Créer" />
            <Button layoutX="57.0" layoutY="339.0" mnemonicParsing="false" onMouseClicked="#onUpdateVehiculeButtonClick" prefHeight="67.0" prefWidth="311.0" text="Mettre à jour le véhicule">
                <tooltip>
                  <Tooltip text="Sélectionnez un véhicule pour modifier son type ; supprimez le véhicule en cas de modification de la plaque" />
               </tooltip></Button>
            <Button layoutX="57.0" layoutY="253.0" mnemonicParsing="false" onMouseClicked="#onDeleteVehiculeButtonClick" prefHeight="67.0" prefWidth="311.0" text="Supprimer" />
            <Label layoutX="84.0" layoutY="101.0" text="Actions sur les véhicules :">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
         </children></Pane>
    </children>
</VBox>
